@model ATP2.FMS.ViewModel.WorkerListModel
@{
    ViewBag.Title = "WorkerList";
    Layout = "~/Views/Shared/AfterLoginOwner.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6">

            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title">Project Workers</h3>
                </div>
                <div class="panel-body">
                    <table class="table table-striped table-hover ">
                        @for(int i=0; i<@Model.Name.Count;i++)
                        {

                            <tr>
                                <td><h4>@Model.Name[i] </h4></td>
                                <td><a href='@Url.Action("WorkerRating", "Owner", new { id = @Model.UserId[i], id2 = @Model.PostId })' class="btn btn-info">Give Rating</a></td>
                            </tr>
                        }
                      
                     
                    </table>


                </div>
            </div>

        </div>

        <div class="col-md-3"></div>
    </div>
    <center><a type="button" href='@Url.Action("Payments", "Owner",new { id = @Model.PostId})' class="btn btn-success">Done</a></center>
</div>
