@model List<FMS.Core.Entities.SkillCategory>
@{
    ViewBag.Title = "Category";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


    <ul class="breadcrumb">
        <li><a href="">Register</a></li>
        <li class="active">Catagory</li>
    </ul>
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            <br/>
            <div class="progress progress-striped active">
                <div class="progress-bar" style="width: 50%"></div>
            </div>
            <form class="form-horizontal">
                <fieldset>
                    <legend>Catagory</legend>
                    <table class="table table-striped table-hover">

                        @for (int i = 0; i < 3; i++)
                        {
                            <tr>

                                @for (int j = 0; j < 3; j++)
                                {
                                    <td><a href="@Url.Action("Skills", "User")/@Model[i * 3 + j].CategoryId">@Model[i * 3 + j].CategoryName</a></td>
                                }
                            </tr>

                        }

                    </table>
                    <br/><br/><br/>
                    <div align="center">
                        <a href="ProfileWorker.html" class="btn btn-success">Finish</a>
                    </div>

                </fieldset>
            </form>

        </div>
        <div class="col-md-3"></div>
    </div>

